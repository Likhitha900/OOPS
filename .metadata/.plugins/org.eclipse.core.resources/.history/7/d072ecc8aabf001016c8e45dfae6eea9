package com.upgraded.StudentReportCard;

public class ArtsStudent extends StudentInfo1
 {
	
	@Override
	public void setGrade() {
		if(Average >= 85.00 && Average<=100.00) {
			this.grade="A";
		}
		else if(Average >= 70.00 && Average<=84.99) {
			this.grade="B";
		}
		else if(Average >=50.00  && Average<=69.99) {
			this.grade="C";
		}
		else if(Average >=35.00 && Average<=49.99) {
			this.grade="D";
		}
		else if(Average<35.00) {
			this.grade="Fail";
		}
		else {
			this.grade="Invalid";
		}
	}
	

}
