package com.upgraded.StudentReportCard;

public class ArtsStudent extends StudentInfo1
 {
	
	
	public ArtsStudent(String sname, String rollNo, double marks1, double marks2, double marks3) {
		super(sname, rollNo, marks1, marks2, marks3);
	}

	@Override
	public void setGrade() {
		if(Average >= 80.00 && Average<=100.00) {
			this.grade="A";
		}
		else if(Average >= 65.00 && Average<=79.00) {
			this.grade="B";
		}
		else if(Average >=50.00  && Average<=64.00) {
			this.grade="C";
		}
		else if(Average >=35.00 && Average<=49.00) {
			this.grade="D";
		}
		else if(Average<35.00) {
			this.grade="Fail";
		}
		else {
			this.grade="Invalid";
		}
	}
	public static void main(String args[]) {
		ArtsStudent Artsstu = new ArtsStudent("Nandini", "k5", 90.00, 91.00, 93.00);
		System.out.println(Artstu.toString());
		
	}

}
