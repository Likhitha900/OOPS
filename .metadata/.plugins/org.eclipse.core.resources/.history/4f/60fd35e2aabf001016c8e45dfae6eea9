package com.upgraded.StudentReportCard;

public class CommerceStudent extends stuSchool{
	@Override
	public void setGrade() {
		if(Average >= 80.00 && Average<=100.00) {
			this.grade="A";
		}
		else if(Average >= 65.00 && Average<=79.00) {
			this.grade="B";
		}
		else if(Average >=50.00  && Average<=64.00) {
			this.grade="C";
		}
		else if(Average >=35.00 && Average<=49.00) {
			this.grade="D";
		}
		else if(Average<35.00) {
			this.grade="Fail";
		}
		else {
			this.grade="Invalid";
		}
	}

}
