package com.Default.constructor;

class studentInfo{
	String stuName;
	int stuId;
	int stuAge;
	String stuLocation;
	
	public studentInfo(String stuName,int stuId,int stuAge,String stuLocation) {
		this.stuName = stuName;
		this.stuId = stuId;
		this.stuAge = stuAge;
		this.stuLocation = stuLocation;
	}
	/*If we declare parameterized or any of the constructor, then compiler removes/does not generate default constructor, 
	So we need to declare default constructor also when we declare any of the constructor
	*/
	public studentInfo() {
		
	}
}
public class Student {
	public static void main(String args[]) {
		studentInfo s1 = new studentInfo();//It will throw compile-time error, if above default constructor is not declared
		studentInfo s2 = new studentInfo("Likhitha", 1, 22, "AP");
		
	}
}
