package com.Default.constructor;

class studentInfo{
	String stuName;
	int stuId;
	int stuAge;
	String stuLocation;
	
	public studentInfo(String stuName,int stuId,int stuAge,String stuLocation) {
		this.stuName = stuName;
		this.stuId = stuId;
		this.stuAge = stuAge;
		this.stuLocation = stuLocation;
	}
}
public class Student {
	public static void main(String args[]) {
		Student s1 = new Student();
		Student s2 = new Student("Likhitha", 1, 22, "AP");
	}
}
