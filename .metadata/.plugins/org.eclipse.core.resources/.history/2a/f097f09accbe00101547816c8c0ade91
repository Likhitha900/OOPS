package com.Default.constructor;

class employeeInfo{
	private String empName;
	private int empId;
	private int empAge;
	private String empLocation;
	
	public employeeInfo(String empName,int empId,int empAge,String empLocation) {
		this.empName = empName;
		this.empId = empId;
		this.empAge = empAge;
		this.empLocation = empLocation;
	}
	
	public employeeInfo() {
		
	}
}

public class InstancePrivateFilede2 {
	public static void main(String args[]) {
		employeeInfo e1 = new employeeInfo();
		employeeInfo e2 = new employeeInfo("Likhitha", 1, 22, "AP");
		System.out.println("Student e1 Age: " + e1.stuAge +"\nStudent e1 Name: "+ e1.stuName +"\nStudent e1 Id: "+e1.stuId+"\nStudent e1 Location: "+e1.stuLocation);
		System.out.println("Student e2 Age: " + e2.stuAge +"\nStudent e2 Name: "+ e2.stuName +"\nStudent e2 Id: "+e2.stuId+"\nStudent e2 Location: "+e2.stuLocation);
		
	}
}
