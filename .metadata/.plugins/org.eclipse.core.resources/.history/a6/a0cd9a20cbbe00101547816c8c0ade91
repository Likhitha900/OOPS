package com.Default.constructor;

privateclass studentInfo{
	String stuName;
	int stuId;
	int stuAge;
	String stuLocation;
	
	public studentInfo(String stuName,int stuId,int stuAge,String stuLocation) {
		this.stuName = stuName;
		this.stuId = stuId;
		this.stuAge = stuAge;
		this.stuLocation = stuLocation;
	}
	
	public studentInfo() {
		
	}
}

